<Window x:Class="Smoothie.EditSurfaceTensionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Edit surface-tension" Height="300" Width="310">
    <DockPanel>
        <ListBox DockPanel.Dock="Top" Name="ListBoxSurfaceTension" Height="230">
        </ListBox>
        <StackPanel DockPanel.Dock="Bottom" Height="90">
            <Button Width="110" Margin="5,5,5,5" Click="ButtonApplyChanges_Click">Apply changes</Button>
        </StackPanel>
    </DockPanel>
</Window>
